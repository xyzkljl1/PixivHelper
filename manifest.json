{
  // 清单文件的版本，这个必须写，而且必须是2
  "manifest_version": 3,
  // 插件的名称
  "name": "PixivHelper",
  // 插件的版本
  "version": "1.0.1",
  // 插件描述
  "description": "PixivHelper",
  // 会一直常驻的后台JS或后台页面
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon/pixiv.png",
    "48": "icon/pixiv.png",
    "128": "icon/pixiv.png"
  },
  // 权限申请
  "permissions": [
    "cookies",
    "tabs", // 标签
    "webRequest" // web请求
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}